<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="connection.autocommit">true</property>
        <property name="hibernate.connection.driver_class">org.postgresql.Driver</property>
        <property name="hibernate.connection.url">jdbc:postgresql://localhost:5432/postgres</property>
        <property name="hibernate.connection.username">OlKir</property>
        <property name="hibernate.connection.password"/>
        <property name="show_sql">true</property>
        <property name="dialect">org.hibernate.dialect.PostgreSQL95Dialect</property>


        <mapping class="com.example.application.models.PlansEntity" />
        <mapping class="com.example.application.models.ClientsEntity" />
        <mapping class="com.example.application.models.OptionsEntity" />
        <mapping class="com.example.application.models.ContractsEntity" />
    </session-factory>
</hibernate-configuration>